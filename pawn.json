{
	"user": "ZantetsukenGT",
	"repo": "DTB",
	"entry": "dtb.pwn",
	"output": "gamemodes/dtb.amx",
	"dependencies": [
		"Zeex/samp-plugin-crashdetect",
		"katursis/Pawn.RakNet@omp",
		"pawn-lang/samp-stdlib",
		"pawn-lang/YSI-Includes@5.x",
		"Southclaws/samp-whirlpool",
		"Southclaws/zcmd",
		"Y-Less/sscanf",
		"oscar-broman/samp-weapon-config",
		"ZantetsukenGT/SA-MP-MySQL"
	],
	"local": true,
	"runtime": {
		"version": "openmp",
		"mode": "server",
		"rcon_password": "changeme",
		"port": 8888,
		"hostname": "defend the bombsite",
		"maxplayers": 50,
		"language": "",
		"mapname": "San Andreas",
		"weburl": "discord.io/attackdefend",
		"gamemodetext": "Unknown",
		"announce": true,
		"lanmode": false,
		"query": true,
		"rcon": false,
		"logqueries": false,
		"sleep": 1,
		"maxnpc": 0,
		"stream_rate": 500,
		"stream_distance": 400,
		"onfoot_rate": 1,
		"incar_rate": 1,
		"weapon_rate": 1,
		"chatlogging": true,
		"timestamp": true,
		"messageholelimit": 3000,
		"messageslimit": 500,
		"ackslimit": 3000,
		"playertimeout": 4000,
		"minconnectiontime": 0,
		"lagcompmode": 1,
		"connseedtime": 300000,
		"db_logging": false,
		"db_log_queries": false,
		"conncookies": true,
		"cookielogging": false,
		"output": true
	},
	"builds": [
		{
			"name": "main",
			"args": [
				"-d3",
				"-O1",
				"-;+",
				"-(+",
				"-\\+",
				"-Z+"
			],
			"includes": [
				"legacy-includes"
			],
			"compiler": {}
		},
		{
			"name": "release",
			"args": [
				"-d0",
				"-O1",
				"-;+",
				"-(+",
				"-\\+",
				"-Z+"
			],
			"includes": [
				"legacy-includes"
			],
			"compiler": {}
		}
	]
